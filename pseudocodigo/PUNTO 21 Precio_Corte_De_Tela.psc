algoritmo Precio_Corte_De_Tela
	
	definir cantidad, precio, monto como real;
	
	Escribir " ingrese la cantidad de tela (Mt) ";
	leer cantidad;
	Escribir " ingrese el precio de la tela (Mt) ";
	Leer precio;
	
	
	monto<-(cantidad*precio);
	
	Escribir " el total del monto a pagar por la cantidad de tela es: ", monto ;
	
	
	
FinAlgoritmo
	

