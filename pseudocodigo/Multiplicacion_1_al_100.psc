Algoritmo Multiplicacion_1_al_100
	definir n,multi como real;
	Multi=1;
	Escribir "----------------------------------------------------------------";
	Escribir "                 Multiplicacion Del 1 Al 100                           ";
	Escribir "----------------------------------------------------------------";
	Para n<-1 Hasta 100 Con Paso 1 Hacer
		multi<-multi*n;
	Fin Para
	Escribir "----------------------------------------------------------------";
	Escribir "               La multiplicacion de 1 - 100 es... ";
	Escribir  "                          " multi;
	Escribir "----------------------------------------------------------------";
	
FinAlgoritmo
